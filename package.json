{
  "name": "misskey-bot",
  "version": "0.0.1",
  "description": "A bot to crosspost content to Misskey",
  "main": "dist/src/index.js",
  "author": "Sam Smucny",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src/**/*.ts --exec ts-node ./src/app.ts",
    "build": "tsc",
    "start": "node ./dist/src/app.js",
    "lint": "eslint --fix --quiet src/**/*.ts",
    "format": "prettier --loglevel silent --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^5.46.0",
    "dotenv": "^16.0.3",
    "misskey-js": "^0.0.14",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.12",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "typescript-eslint-parser": "^22.0.0"
  }
}
